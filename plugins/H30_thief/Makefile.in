
CC       = @CC@
DEFS     = @PICFLAG@ @DEFS@ -D@OS@

CFLAG    = @CFLAGS@
COPTS    = -I../..

PROG     = H30_thief

ALLOPT   = $(CFLAG) $(COPTS)


all: $(PROG)

$(PROG):
	$(CC) $(ALLOPT) $(DEFS) $(PROG).c -o ec_$(PROG).so
	@echo
	@echo "@SB@$(PROG) plugin is ready...@EB@"
	@echo

clean:
	rm -f *.bak
	rm -f ec_$(PROG).so

distclean: clean
	rm -f Makefile

